{
    "manifest_version": 3,
    "name": "arxml viewer",
    "description": "",
    "version": "1.0.0",
    "icons": {
      "16": "icon-16x16.png",
      "32": "icon-32x32.png",
      "128": "icon-128x128.png"
    },
    "content_scripts": [
      {
        "js": [
          "content_scripts.js"
        ],
        "css": [
            "style.css"
        ],
        "matches": [
            "file://*.arxml"
        ]
      }
    ]
  }